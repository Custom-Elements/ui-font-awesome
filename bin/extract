#!/usr/bin/env awk -f
BEGIN {
	FS=":";
	npm install --save ui-toolkit
}
{
	if (match($1, "@fa-var")) {
		printf("icon[%s], [icon=\"%s\"]\n", substr($1, 9), substr($1, 9));
		print "{";
		print "  &:before {";
		print "    position: absolute;"
		print "    text-align: center;"
		print "    top: 0; left: 0; right: 0; bottom: 0;"
		print "    font-family: FontAwesome;"
		print "    content:", $2;
		print "  }";
		print "}";
	}
}
END {

}
